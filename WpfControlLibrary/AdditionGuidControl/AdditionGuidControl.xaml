<UserControl x:Class="WpfControlLibrary.AdditionGuidControl.AdditionGuidControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfControlLibrary.AdditionGuidControl"
             mc:Ignorable="d">
    <CheckBox Name="GuidName" IsChecked="{Binding GuidChecked, Mode=TwoWay}" Content="{Binding Model.Guidfullname}"/>
</UserControl>
